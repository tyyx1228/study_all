	ATTRIBUTE7  VARCHAR(500)  comment  '自定义字段7',
	ATTRIBUTE8  VARCHAR(500)  comment  '自定义字段8',
	ATTRIBUTE9  VARCHAR(500)  comment  '自定义字段9',
	ATTRIBUTE10  VARCHAR(500)  comment  '自定义字段10',
	ATTRIBUTE11  VARCHAR(500)  comment  '自定义字段11',
	ATTRIBUTE12  VARCHAR(500)  comment  '自定义字段12',
	ATTRIBUTE13  VARCHAR(500)  comment  '自定义字段13',
	ATTRIBUTE14  VARCHAR(500)  comment  '自定义字段14',
	ATTRIBUTE15  VARCHAR(500)  comment  '自定义字段15',
	ATTRIBUTE16  VARCHAR(500)  comment  '自定义字段16',
	ATTRIBUTE17  VARCHAR(500)  comment  '自定义字段17',
	ATTRIBUTE18  VARCHAR(500)  comment  '自定义字段18',
	ATTRIBUTE19  VARCHAR(500)  comment  '自定义字段19',
