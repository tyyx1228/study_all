	G_ID  VARCHAR(500)  comment  '机组号',
	PIKEY  VARCHAR(36)  comment  'PI数据库编码',
	BZ_WARN  VARCHAR(100)  comment  '班组级报警',
	DC_WARN  VARCHAR(100)  comment  '电厂级报警',
	BM_WARN  VARCHAR(100)  comment  '部门级报警',
	JT_WARN  VARCHAR(100)  comment  '集团级报警',
	BJ_SYSTEM  VARCHAR(100)  comment  '系统',
	BZ_ENDVALUE  VARCHAR(100)  comment  '班组级报警返回值',
	DC_ENDVALUE  VARCHAR(100)  comment  '电厂级报警返回值',
	BM_ENDVALUE  VARCHAR(100)  comment  '部门级报警返回值',
	JT_ENDVALUE  VARCHAR(100)  comment  '集团级报警返回值',
