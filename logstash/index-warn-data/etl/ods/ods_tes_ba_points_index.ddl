	UP_P_I_ID  VARCHAR(36)  comment  '测点父节点ID',
	APP_ID  VARCHAR(36)  comment  '设备台账ID',
	KKS_CODE  VARCHAR(50)  comment  'KKS码',
	T_P_CODE  VARCHAR(500)  comment  '测点或者指标编号',
	T_P_NAME  VARCHAR(500)  comment  '测点或者指标名称',
	SPEC_ID  VARCHAR(500)  comment  '所属专业',
	T_P_COMP  VARCHAR(500)  comment  '权限标识',
	DCS_NO  VARCHAR(500)  comment  'DCS编码',
	P_DATA_LEVEL  VARCHAR(500)  comment  '数据级别',
	ORG_ID  VARCHAR(50)  comment  '组织id',
	T_P_UNIT  VARCHAR(500)  comment  '单位',
	T_P_ORIGIN  VARCHAR(500)  comment  '来源',
	AID_KKS_CODE  VARCHAR(36)  comment  '辅助参数KKS码',
