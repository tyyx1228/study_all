	W_ASSESS  VARCHAR(500)  comment  '报警考核',
	W_DISCERN  VARCHAR(500)  comment  '数据识别、有效性判断',
	IS_POINTS  VARCHAR(26)  comment  'P:测点 I:指标',
	CREATE_BY  VARCHAR(36)  comment  '创建人',
	CREATE_DATE  DATE  comment  '创建日期',
	LAST_UPDATED_BY  VARCHAR(36)  comment  '修改人',
